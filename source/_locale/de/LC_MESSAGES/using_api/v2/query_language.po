msgstr "Der Datensatz-Veröffentlicher"

#: ../../source/using_api/v2/query_language.rst:61
msgid "title"
msgstr "Titel"

#: ../../source/using_api/v2/query_language.rst:62
msgid "The dataset title"
msgstr "Der Datensatz-Titel"

#: ../../source/using_api/v2/query_language.rst:63
msgid "description"
msgstr "Beschreibung"

#: ../../source/using_api/v2/query_language.rst:64
msgid "The dataset description"
msgstr "Die Datensatz-Beschreibung"

#: ../../source/using_api/v2/query_language.rst:65
msgid "license"
msgstr "Lizenz"

#: ../../source/using_api/v2/query_language.rst:66
msgid "The dataset license"
msgstr "Die Datensatz-Lizenz"

#: ../../source/using_api/v2/query_language.rst:67
msgid "records_count"
msgstr "records_count"

#: ../../source/using_api/v2/query_language.rst:68
msgid "The number of records in the dataset"
msgstr "Die Anzahl der Einträge im Datensatz"

#: ../../source/using_api/v2/query_language.rst:69
msgid "modified"
msgstr "geändert"

#: ../../source/using_api/v2/query_language.rst:70
msgid "The last modification date of the dataset"
msgstr "Das Datum der letzten Änderung des Datensatzes"

#: ../../source/using_api/v2/query_language.rst:71
msgid "language"
msgstr "Sprache"

#: ../../source/using_api/v2/query_language.rst:72
msgid "The language of the dataset (iso code)"
msgstr "Die Sprache des Datensatzes (ISO-Code)"

#: ../../source/using_api/v2/query_language.rst:73
msgid "theme"
